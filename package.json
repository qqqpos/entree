{
  "name": "unitedpos",
  "version": "0.9.7",
  "require": "0.9.7",
  "stage": "Beta",
  "description": "UnitedPOS point of sale system",
  "author": "United POS <huangxuewu@gmail.com>",
  "license": "Apache-2.0",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "electron-builder install-app-deps",
    "rebuild": "electron-rebuild -f -w serialport",
    "release": "npm run build && npm run publish"
  },
  "updater": {
    "url": "https://raw.githubusercontent.com/huangxuewu/entree/master/updates.json"
  },
  "build": {
    "productName": "Entree POS",
    "appId": "unitedpos.software.pos.beta",
    "copyright": "Copyright Â© 2017 ${author}",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "win": {
      "icon": "build/icons/icon.ico",
      "target": "squirrel"
    },
    "squirrelWindows": {
      "iconUrl": "https://raw.githubusercontent.com/huangxuewu/entree/master/build/icons/icon.ico",
      "loadingGif": "build/icons/install.gif"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "@tweenjs/tween.js": "^17.2.0",
    "amcharts3": "^3.21.12",
    "axios": "^0.18.0",
    "bson-objectid": "^1.2.3",
    "diskusage": "^0.2.4",
    "electron-simple-updater": "^1.2.3",
    "file-saver": "^1.3.3",
    "getmac": "^1.4.0",
    "hammerjs": "^2.0.8",
    "lodash.debounce": "^4.0.8",
    "md5": "^2.2.1",
    "moment": "^2.22.1",
    "net": "^1.0.2",
    "nodemailer": "^4.6.4",
    "randomcolor": "^0.5.3",
    "serialport": "^4.0.7",
    "signature_pad": "^2.3.2",
    "socket.io-client": "^2.1.1",
    "velocity-animate": "^2.0.2",
    "vue": "^2.5.17",
    "vue-avatar": "^2.1.6",
    "vue-electron": "^1.0.6",
    "vue-router": "^2.8.1",
    "vue-slicksort": "^0.1.10",
    "vue-socket.io": "^2.1.1",
    "vue-touch": "^2.0.0-beta.4",
    "vuebars": "^0.1.1",
    "vuedraggable": "^2.16.0",
    "vuetrend": "^0.2.3",
    "vuex": "^2.5.0",
    "wake_on_lan": "0.0.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.1",
    "cfonts": "^1.1.3",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^4.4.2",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.11",
    "del": "^2.2.2",
    "devtron": "^1.4.0",
    "electron": "^2.0.5",
    "electron-builder": "^20.26.0",
    "electron-builder-squirrel-windows": "^20.26.0",
    "electron-debug": "^2.0.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-rebuild": "^1.8.1",
    "electron-simple-publisher": "^0.4.12",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.7",
    "multispinner": "^0.2.1",
    "node-gyp": "^3.7.0",
    "style-loader": "^0.21.0",
    "url-loader": "^0.5.7",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^12.2.2",
    "vue-style-loader": "^3.1.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.11.2",
    "webpack-hot-middleware": "^2.22.3"
  }
}
